/*************** Below style are for links in the home page of staff users *********************/


/* [id] refers to the standard browser only, 
IE will ignore this style property, 
for IE separate property is written below. */ 

#outer[id] 
{
	width:160px;
	height:160px;
	margin: 10px 5px 10px 5px;
	float:left;
	display: table; 
	position: static;
	outline:none;
}

#outer[id] a
{
	outline:none;
}

#middle[id] 
{
	width: 150px;
	height: 150px;
	padding: 0 12px 0 12px;
	font-size:12px;
	color:#8a053d;
	table-layout:fixed; 
	overflow-x:hidden; 
	overflow-y:hidden;
	cursor:hand;
	display: table-cell; 
	vertical-align: middle;
	left: auto;
	position:static;
	background: url(../../images/custom1/batch_button4.png) center center no-repeat;
}

#middle[id]:hover
{	
	background: url(../../images/custom1/batch_button5.png) center center no-repeat;
}

#inner[id] 
{
	position: static;
} 

