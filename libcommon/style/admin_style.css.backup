	color:#FFFFFF;
}

.fromList_long th a:hover
{
	color:#CCCCCC;
}

/************* style sheet *************/
div#qTip {
 padding: 5px;
 width:250px;
 border: 1px solid #fff;
 border-right-width: 1px;
 border-bottom-width: 1px;
 display: none;
 background: #464646;
 color: #FFF;
 font: 12px Verdana, Arial, sans-serif;
 text-align: left;
 position: absolute;
 z-index: 1000;
}

.tooltip
{
	text-decoration:none;
	color:#000;
}

