.fromList_long
{
	width:950px;
	table-layout:fixed;
	overflow:hidden;
}

.fromList_long td
{
	height:25px;
	color:#000;
	overflow:hidden;	
	background-color:#EEE;
}

.fromList_long th
{
	height:25px;
	color:#FFFFFF;
	overflow:hidden;
	text-transform:capitalize;
