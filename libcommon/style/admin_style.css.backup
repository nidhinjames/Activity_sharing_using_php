/* CSS Document */

 
html, body {
	margin: 0 0 0 0;
	background : url('../images/admin_images/bg2.jpg') #fff repeat-x;
	font-family : 'verdana' , arial, tahoma, sans-serif;
	font-size : 12px;
}


h1 
{
color:#217ba5; 
text-transform:capitalize;
font-size:20pt; 
font-family: verdana, arial, 'sans serif';
}

h2 
{
color:#3399CC; 
font-size:14pt; 
font-family: verdana, arial, 'sans serif';
}

h3 
{
color:#006699; 
font-size:13px; 
font-weight: bold; 
font-family: verdana, arial, 'sans serif';
}

hr {color:navy}


p
{
	margin:5px 5px 5px 25px;
	color:#000; 
	font-size:11px; 
	font-weight:normal;
	font-family: verdana, arial, 'sans serif';
}

.mainTable
{
	width: 100%;
}

.pageTab
{
	width: 995px;
}

.topNav
{
	width:100%;
	height:20px;
	text-align:right;
	background-color:#000000;
}

.topNav a
{	padding-left:20px;
	color:#FFFFFF;
	font-weight:bold;
	text-decoration:none;
}

.topNav a:hover
{
	color:#CCCCCC;
}

.pageTab #heading
{
	height: 101px;
}

.pageTab #heading #caption 
{
	margin: 20px 0px 0px 10px;
	font-family: 'Verdana' , Arial, Helvetica, sans-serif;
	color:#FFFFFF;
	font-size:20px;
	float:left;
}

.pageTab #heading #caption #subcap
{
	color:#FFFFFF;
	font-size:12px;
	float:left;
}

.pageTab #foottxt
{
	color:#FFFFFF;
	font-size:12px;
	text-align:center;
}

.foot
{
	height:30px;
	background: #464646;
}

/************ login table *********/

.login_tab
{
	width: 300px;
    font-size:12px;	
	border: 3px solid #0b89b2;
}

.login_tab #head
{
	width:100%;
	height:25px;
	color:#FFFFFF;
	background-color:#464646;
}

.login_tab #error
{	
	width:100%;
	height:30px;
	color:#FF0000;
}

.login_tab th
{
	padding-left:15px;
	height:20px;
	background-color:#EEE;	
}

.login_tab #left
{
	padding-left:15px;
	width:100px;
	height:20px;
	background-color:#EEE;	
}

.login_tab #right
{
	padding-left:5px;
	width:200px;
	height:20px;
	background-color:#EEE;	
}

/********* tables ******************/

.welcometxt
{
	padding-top:150px;
	width:450px;
	color:#217ba5; 
	text-transform:capitalize;
	font-size:20pt; 
	font-family: verdana, arial, 'sans serif';	
}



.formInput
{
	width:750px;	
}

.formInput th
{		
	width:100%;
	height:25px;
	color:#FFFFFF;
	text-transform:capitalize;
	background-color:#464646;
}

.formInput td
{		
	padding: 0px 0px 0px 20px;
	height:25px;
	color:#000;
	vertical-align:middle;
	text-transform:capitalize;
	background-color:#EEE;
}

div.extra_head
{
	width:745px;
	height:35px;
	padding-top:10px;
	margin-left:auto;
	margin-right:auto;
	text-align:center;
	vertical-align:middle;
	position:relative;
	color:#FFF;
	font-weight:bold;
	background-color:#464646;
}

/*******************************************/
div.extra_foot_dd
{
	width:745px;
	height:35px;
	padding:0;
	margin-left:auto;
	margin-right:auto;
	text-align:right;
	vertical-align:middle;
	position:relative;
	color:#FFF;
	font-weight:bold;
	background-color:#464646;
	border: 1px solid #666;
}

div.extra_foot_dd a
{
	text-decoration:none;
	color:#FFF;
}

div.extra_foot_dd a:hover
{
	text-decoration:none;
	color:#0b89b2;
}

div.extra_foot_dd p
{
	padding-top:2px;
	font-weight:bold;
}

/*********************************************************/

span.element
{
	margin-left:25px;	
}

div.extra_foot
{
	width:745px;
	height:35px;
	padding:0;
	margin-left:auto;
	margin-right:auto;
	text-align:center;
	vertical-align:middle;
	position:relative;
	color:#FFF;
	font-weight:bold;
	background-color:#464646;
	border: 1px solid #666;
}

div.extra_foot p
{
	padding-top:2px;
	font-weight:bold;
	/*color:#FFF;
	background-color:#464646;*/
}

div.extra_foot a
{
	text-decoration:none;
	color:#FFF;
}

div.extra_foot a:hover
{
	text-decoration:none;
	color:#0b89b2;
}

div.staff_head
{
	width:745px;
	height:60px;
	padding-top:10px;
	margin-left:auto;
	margin-right:auto;
	text-align:center;
	vertical-align:middle;
	position:relative;
	color:#FFF;
	font-weight:bold;
	background-color:#464646;
}

.fromList
{
	width:750px;
	table-layout:fixed;
	overflow:hidden;
}

.fromList td
{
	height:25px;
	color:#000;
	overflow:hidden;	
	background-color:#EEE;
}

.fromList th
{
	height:25px;
	color:#FFFFFF;
	overflow:hidden;
	text-transform:capitalize;
	background-color:#464646;
}

.fromList th a
{
	font-size:12px;
	text-decoration:none;
	color:#FFFFFF;
}

.fromList th a:hover
{
	color:#CCCCCC;
}

.caption
{
	width:50%;
	height:30px;
	margin: 25px 0px 0px 250px;
	padding: 10px 0px 0px 0px;
	text-align:center;
	text-transform:capitalize;
	float:none;
	font-family:Verdana, Geneva, 'sans-serif';
	font-size:14px;
	font-weight:bold;
	color:#464646;
	background:url(../images/admin_images/admin_head.png) no-repeat center center #fff;
	/*
	border-top: 1px solid #999;
	border-bottom: 1px solid #999;
	*/
	display:block;
}

/****************************************************************/

div.del_msg
{
	width:350px;
	height:auto;
	margin-left:auto;
	margin-right:auto;
	text-align:center;
	vertical-align:middle;
	background-color:#EEE;
	border: 3px solid #464646;
}

span.del_msg_head
{
	width:350px;
	height:35px;
	padding-top:5px;
	margin-bottom:25px;
	text-align:center;
	vertical-align:middle;
	font-size:12px;
	font-weight:bold;
	color:#FFF;
	background-color:#464646;
	display:block;	
	
}

.del_msg_foot
{
	width:350px;
	height:25px;
	padding-top:5px;
	margin-top:25px;
	text-align:center;
	vertical-align:middle;
	font-size:12px;
	font-weight:bold;
	color:#FFF;
	background-color:#464646;
	display:block;	
}

/************************** Exam panel *********************/

.examInput
{
	width:749px;	
	margin-left:auto;
	margin-right:auto;	
	text-align:center;
}

.examInput th
{		
	width:100%;
	height:33px;
	vertical-align:middle;
	color:#FFFFFF;
	text-transform:capitalize;
	background-color:#464646;
}

.examInput th.foot
{		
	width:100%;
	height:35px;
	vertical-align:middle;
	color:#FFFFFF;
	text-transform:capitalize;
	background-color:#464646;
}

.examInput td
{		
	padding: 0px 0px 0px 20px;
	width:50%;
	height:25px;
	color:#000;
	text-align:left;
	vertical-align:middle;
	text-transform:capitalize;
	background-color:#EEE;
}

.exam_indent
{
	width:760px; 
	padding-top:10px; 
	margin-left:auto; 
	margin-right:auto; 
	text-align:center; 
	vertical-align:middle; 
}

/***************** Timetable panel *************/

div.tt_head
{
	width:990px;
	height:60px;
	padding-top:10px;
	margin-left:auto;
	margin-right:auto;
	text-align:center;
	vertical-align:middle;
	position:relative;
	color:#FFF;
	font-weight:bold;
	background-color:#464646;
}

.timetableList
{
	width:990px;
	height:500px
	table-layout:fixed;
	overflow:auto;	
	background-color: #fff;
	border: 1px solid #666;
}

.timetableList td
{
	width:100px;
	height:45px;
	color:#000;
	overflow:hidden;	
	text-align:center;
	background-color:#e4e4e4;
}

.timetableList td p
{
	
	color:#000;
	overflow:hidden;	
	text-align:left;
	float:left;
	background-color:#e4e4e4;
}

.timetableList td.days
{
	
	height:45px;
	color:#000;
	overflow:hidden;	
	background-color:#ccc;
}

.timetableList th
{
	height:45px;
	font-size:10px;
	color:#FFFFFF;
	overflow:hidden;
	text-transform:capitalize;
	background-color:#464646;
}

.timetableList th a
{
	font-size:12px;
	text-decoration:none;
	color:#FFFFFF;
}

.timetableList th a:hover
{
	color:#CCCCCC;
}

div.timetableList_foot
{
	width:100%;
	height:35px;
	padding:10px 0 5px 0;
	margin-left:auto;
	margin-right:auto;
	text-align:center;
	vertical-align:middle;
	position:relative;
	color:#FFF;
	background-color:#464646;
	/*font-weight:bold;	
	border: 1px solid #666;*/
}

p.tt_result
{
	margin-top:50px;
	margin-bottom:25px;
	margin-left:auto;
	margin-right:auto;
	text-align:center;
	font-size:16px;
	color:#0a88b1;
	
}

div.tt_headfor_print
{
	width:100%;
	height:30px;
	padding-top:30px;
	margin-left:auto;
	margin-right:auto;
	text-align:center;
	vertical-align:middle;
	position:relative;
	color:#FFF;
	font-weight:bold;
	background-color:#464646;
}

